def swap_case(s):
    a=''
    for i in range(len(s)):
        if s[i].isupper()==True:
            a+=s[i].lower()
        elif s[i].islower()==True:
            a+=s[i].upper()
        else:
            a+=s[i]

    return a

if __name__ == '__main__':
    s = input()
    result = swap_case(s)
    print(result)
